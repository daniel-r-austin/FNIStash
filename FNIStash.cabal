name: FNIStash
version: 0.0.1
cabal-version: >=1.2
build-type: Simple
license: AllRightsReserved
license-file: ""
description:  
data-dir: ""
 
executable FNIStash
    build-depends: QuickCheck -any, base -any, binary -any,
                   binary-strict -any, bytestring -any, containers -any, errors -any,
                   monad-loops -any, text -any, tuple -any
    main-is: Main.hs
    buildable: True
    hs-source-dirs: src
    other-modules: FNIStash.Logic.File FNIStash.Logic.Data
                   FNIStash.Logic.Crypto
 
test-suite test-FNIStash
    build-depends: QuickCheck -any, base -any, binary -any,
                   binary-strict -any, bytestring -any, containers -any, errors -any,
                   monad-loops -any, text -any, tuple -any
    type: exitcode-stdio-1.0
    main-is: Main.hs
    buildable: True
    cpp-options: -DMAIN_FUNCTION=testMain
    hs-source-dirs: src